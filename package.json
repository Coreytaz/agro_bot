{
  "name": "agro-bot",
  "version": "1.0.0",
  "description": "agro bot",
  "keywords": [
    "node",
    "typescript",
    "agro",
    "bot",
    "express"
  ],
  "exports": "./src/server.ts",
  "scripts": {
    "build": "node --run validate-typescript && node --run generate-dist",
    "server:prod": "dotenv -e .env -- node dist/server.js",
    "server:dev": "nodemon",
    "lint": "eslint . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "eslint . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint",
    "generate-dist": "swc ./src -d dist --strip-leading-paths",
    "validate-typescript": "tsc -p tsconfig.prod.json",
    "loglevel:change": "runtime-node-refresh"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/parse-mode": "^2.2.0",
    "@grammyjs/runner": "^2.0.3",
    "@libsql/client": "^0.15.15",
    "cron": "^4.3.3",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "grammy": "^1.38.3",
    "grammy-middlewares": "^1.0.11",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "runtime-node-refresh": "^1.0.7",
    "source-map-support": "^0.5.21",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.20",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.7"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.52.3",
    "@swc/core-linux-arm64-musl": "^1.13.20",
    "@swc/core-linux-x64-gnu": "^1.13.20",
    "@swc/core-linux-x64-musl": "^1.13.20"
  }
}
